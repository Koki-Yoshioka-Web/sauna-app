<div class="main users-index">
  <div class="container">
    <h1 class="users-heading">サウナerのみんな</h1>
    
    <%@users.each do |user| %>
      <div class="users-index-item">
         <div class="user-left">
          <img src="<%= "/uploads/#{user.image_name}" %>">
        </div>
        <div class="user-right">
          
          <%=link_to(user.name, "/users/#{user.id}") %>
        </div>
      </div>
    <% end%>
  </div>
</div>